type DeviceInfo {
    deviceId: String!
    name: String!
    online: Boolean!
    powerOn: Boolean!
}

type Query {
    getStateKapcsoloFolyoso: DeviceInfo!
}

type Mutation {
    powerOnKapcsoloFolyoso: Boolean!
    powerOffKapcsoloFolyoso: Boolean!
}

type Subscription {
    subscribeDeviceInfoUpdate(deviceId:String!) : DeviceInfo!
}